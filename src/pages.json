{
	"pages": [
		{
			"path": "pages/index/index",
			"style": {
				"navigationBarTitleText": "uni-app"
			}
		}
	],
	"subPackages": [
		{
			"root": "pages/pkgA",
			"pages": [
				{
					"path": "index/a"
				}
			]
		},
		{
			"root": "pages/pkgB",
			"pages": [
				{
					"path": "index/b"
				}
			]
		}
	],
	"preloadRule": {
		"pages/index/index": {
			"network": "all",
			"packages": [
				"pages/pkgA",
				"pages/pkgB"
			]
		}
	},
	"globalStyle": {
		"navigationBarTextStyle": "black",
		"navigationBarTitleText": "uni-app",
		"navigationBarBackgroundColor": "#F8F8F8",
		"backgroundColor": "#F8F8F8"
	}
}
